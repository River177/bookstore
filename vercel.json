{
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx prisma generate",
  "devCommand": "npm run dev",
  "framework": null,
  "outputDirectory": "dist/client",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ]
}

